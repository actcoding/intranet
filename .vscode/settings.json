{
    "files.associations": {
        "artisan": "php",
        "composer": "shellscript"
    },

    "explorer.autoRevealExclude": {
        "**/node_modules": true,
        "**/vendor": true
    },

    "editor.defaultFormatter": null,
    "editor.linkedEditing": true,
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": "explicit"
    },

    "i18n-ally.localesPaths": [
        "frontend/messages"
    ]
}
